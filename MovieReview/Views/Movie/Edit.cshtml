
@model Movie;

<h1>Movie Register</h1>

<form method="post">


    <div class="mb-3">
      <label for="title" class="form-label">Titulo</label>
      <input type="text" asp-for="Title" class="form-control" id="title" placeholder="name@example.com">
    </div>
    <div class="mb-3">
      <label for="Price" class="form-label">Descrição</label>
      <input type="number" asp-for="Price" />
    </div>

    <input type="submit" />
</form>